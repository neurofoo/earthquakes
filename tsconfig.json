{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "lib": ["esnext", "dom", "dom.iterable"],
        "removeComments": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "allowJs": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "baseUrl": "/"
    },
    "include": ["./src", "./webpack.config.ts"]
}
